{
  "files.exclude": {
    "**/.DS_Store": false,
    "**/.git": false,
    "**/.hg": false,
    "**/.svn": false,
    "**/Thumbs.db": false
  },
  "files.associations": {
    "*.asset": "yaml",
    "*.meta": "yaml",
    "*.prefab": "yaml",
    "*.unity": "yaml"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.sln": "*.csproj",
    "*.slnx": "*.csproj"
  },
  "dotnet.defaultSolution": "unity_git_vcs.slnx"
}
